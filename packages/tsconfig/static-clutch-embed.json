{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Static Clutch Embed",
  "extends": "./base.json",
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./dist",
    "declaration": true
  },
  "exclude": ["node_modules"]
}
